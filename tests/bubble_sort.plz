int temp.
int i.
int j.
int arr(10) = {4,3,2,1,5,7,6,8,9,10}.


sub void bubbleSort(int n):

    for i from 0 by 1 while i < n:
        for j from 0 by 1 while j < n-i-j:
            if arr[j] > arr[j+1] :
                temp = arr[j].
                arr[j] = arr[j+1].
                arr[j+1] = temp.
            end
        end
    end
end

sub int find_element(int n):
    for i from 0 by 1 while i < n:
        if arr[i] == n:
            return i.
        end
    end
    return -1.
end

